\u0026lt;tr\u0026gt;\u003Cbr \/\u003E    \u0026lt;td\u0026gt;No.\u0026lt;\/td\u0026gt;\u003Cbr \/\u003E    \u0026lt;td\u0026gt;Nama Gambar\u0026lt;\/td\u0026gt;\u003Cbr \/\u003E    \u0026lt;td\u0026gt;Aksi\u0026lt;\/td\u0026gt;\u003Cbr \/\u003E  \u0026lt;\/tr\u0026gt;\u003Cbr \/\u003E\u0026lt;?php \u003Cbr \/\u003E$querygambar=mysql_query(\"select * from upload\");\u003Cbr \/\u003E\/\/ Perulangan untuk menampilkan data gambar\u003Cbr \/\u003Ewhile ($data=mysql_fetch_array($querygambar))\u003Cbr \/\u003E{ $a++;\u003Cbr \/\u003E?\u0026gt;\u003Cbr \/\u003E  \u0026lt;tr\u0026gt;\u003Cbr \/\u003E   \u0026lt;!-- Untuk menampilkan nomor urut --\u0026gt;\u003Cbr \/\u003E    \u0026lt;td\u0026gt;\u0026lt;?=$a;?\u0026gt;\u0026lt;\/td\u0026gt;\u003Cbr \/\u003E    \u0026lt;!-- untuk menampilkan nama gambar --\u0026gt;\u003Cbr \/\u003E    \u0026lt;td\u0026gt;\u0026lt;?=$data['nama_gambar'];?\u0026gt;\u0026lt;\/td\u0026gt;\u003Cbr \/\u003E    \u0026lt;!-- membuat link yang mengarah pada lihat.php untuk melihat gambar yang diupload --\u0026gt;\u003Cbr \/\u003E    \u0026lt;td\u0026gt;\u0026lt;a href=\"lihat.php?idgambar=\u0026lt;?=$data['id_gambar'];?\u0026gt;\"\u0026gt;Lihat Gambar\u0026lt;\/a\u0026gt;\u0026lt;\/td\u0026gt;\u003Cbr \/\u003E  \u0026lt;\/tr\u0026gt;\u003Cbr \/\u003E\u0026lt;?php } ?\u0026gt;\u003Cbr \/\u003E\u0026lt;\/table\u0026gt;\u003Cbr \/\u003E\u003Cbr \/\u003E\u0026lt;\/body\u0026gt;\u003Cbr \/\u003E\u0026lt;\/html\u0026gt;\u003Cbr \/\u003E\u003C\/pre\u003E\u003Cbr \/\u003E5. Buatlah sebuah file dengan nama lihat.php dan ketikan script dibawah ini\u003Cbr \/\u003E\u003Cbr \/\u003E\u003Cpre class=\"brush:html\"\u003E\u0026lt;?php\u003Cbr \/\u003E$server=\"localhost\"; \/\/lokasi server\u003Cbr \/\u003E$username=\"root\";  \/\/ username mysql\u003Cbr \/\u003E$password=\"root\"; \/\/ jika mysql anda tidak diberi password silahkan dikosongkan\u003Cbr \/\u003E$namadb=\"latihan\"; \/\/ Nama database\u003Cbr \/\u003E\u003Cbr \/\u003E\/\/ Mengkoneksikan file index.php ke server\u003Cbr \/\u003E$conn=mysql_connect($server,$username,$password);\u003Cbr \/\u003E\u003Cbr \/\u003E\/\/ Memilih database\u003Cbr \/\u003E$db=mysql_select_db($namadb);\u003Cbr \/\u003E\u003Cbr \/\u003E\/\/ mendapakan value dari url ?lihat.php?idgambar=\u003Cbr \/\u003E$id=$_GET['idgambar'];\u003Cbr \/\u003E\/\/ Query untuk menampilkan data dari table upload\u003Cbr \/\u003E$query=mysql_query(\"select * from upload where id_gambar='\".$id.\"'\");\u003Cbr \/\u003E$data=mysql_fetch_array($query)\u003Cbr \/\u003E?\u0026gt;\u003Cbr \/\u003E\u0026lt;html\u0026gt;\u003Cbr \/\u003E\u0026lt;head\u0026gt;\u003Cbr \/\u003E\u0026lt;title\u0026gt;[Tutorial_Koding] Uplaod Gambar dengan PHP dan MySQL\u0026lt;\/title\u0026gt;\u003Cbr \/\u003E\u0026lt;\/head\u0026gt;\u003Cbr \/\u003E\u0026lt;body\u0026gt;\u003Cbr \/\u003E\u0026lt;!-- menampilkan gambar --\u0026gt;\u003Cbr \/\u003E\u0026lt;img src=\".\/image\/\u0026lt;?=$data['nama_gambar'];?\u0026gt;\" width=\"500\" height=\"500\"\u0026gt;\u003Cbr \/\u003E\u0026lt;\/body\u0026gt;\u003Cbr \/\u003E\u0026lt;\/html\u0026gt;\u003Cbr \/\u003E\u003C\/pre\u003E\u003Cbr \/\u003E6. Buka browser anda dan ketikan\u0026nbsp;http:\/\/localhost\/uploadgambar\/\u003Cbr \/\u003E\u003Cbr \/\u003E\u003Cdiv class=\"separator\" style=\"clear: both; text-align: center;\"\u003E\u003Ca href=\"http:\/\/2.bp.blogspot.com\/-XpJrm9Y5wps\/U2OasrbtktI\/AAAAAAAAAiY\/4v0yGQe0va8\/s1600\/Upload+dan+Menampilkan+Gambar+dengan+PHP+dan+MySQL+2.png\" imageanchor=\"1\" style=\"margin-left: 1em; margin-right: 1em;\"\u003E\u003Cimg alt=\"Upload dan Menampilkan Gambar dengan PHP dan MySQL\" border=\"0\" src=\"http:\/\/2.bp.blogspot.com\/-XpJrm9Y5wps\/U2OasrbtktI\/AAAAAAAAAiY\/4v0yGQe0va8\/s1600\/Upload+dan+Menampilkan+Gambar+dengan+PHP+dan+MySQL+2.png\" height=\"194\" title=\"Upload dan Menampilkan Gambar dengan PHP dan MySQL\" width=\"320\" \/\u003E\u003C\/a\u003E\u003C\/div\u003E\u003Cblockquote class=\"tr_bq\"\u003EKlik tombol pilih file lalu klik tombol upload 